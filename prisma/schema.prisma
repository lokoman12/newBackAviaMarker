generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model SCOUT {
  Serial   String    @id @db.VarChar(10)
  GarNum   String?   @db.VarChar(20)
  DateTime DateTime? @db.DateTime(0)
  t_obn    Float?
  Lat      Float?
  Lon      Float?
  Speed    Float     @default(0) @db.Float
  Course   Float     @default(0) @db.Float
  DigIO    Int?      @default(0) @db.SmallInt
  Adc0     Int?      @default(0) @db.SmallInt
  Adc1     Int?      @default(0) @db.SmallInt
  Stat0    Int?      @default(0) @db.SmallInt
  Stat1    Int?      @default(0) @db.SmallInt
  Data     String?   @db.VarChar(250)

  @@index([GarNum], map: "GarNum")
  @@index([t_obn], map: "t_obn")
}

