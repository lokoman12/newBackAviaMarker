model alarmAM {
  id            Int      @id @default(autoincrement()) @db.UnsignedInt
  lat           Float    @db.Float
  lon           Float    @db.Float
  speed         Float    @db.Float
  time          DateTime
  typeViolation String   @db.VarChar(255)
  name          String   @db.VarChar(50)
}

model line {
  id                   Int       @id @default(autoincrement()) @db.UnsignedInt
  name                 String?   @db.VarChar(50)
  time                 DateTime?
  coordinates__string_ String?   @map("coordinates: string;") @db.VarChar(50)
  distance             Int?      @db.UnsignedInt
  description          String?   @db.VarChar(100)
  project              String?   @db.VarChar(50)
  mode                 String?   @db.VarChar(50)
}

model positionAM {
  id     Int    @id @default(autoincrement()) @db.UnsignedInt
  lat    Float  @db.Float
  lon    Float  @db.Float
  name   String @db.VarChar(100)
  speed  Float  @db.Float
  time   String @db.VarChar(100)
  status String @db.VarChar(100)
}

model rdd {
  id       Int    @id @default(autoincrement()) @db.UnsignedInt
  id_vpp   Int    @db.UnsignedTinyInt
  id_rd    Int    @db.UnsignedTinyInt
  name_rd  String @db.VarChar(60)
  close_rd Int    @db.UnsignedInt
  airport  String @db.VarChar(60)

  @@index([airport], map: "airport")
}
